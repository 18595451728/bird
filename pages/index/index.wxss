  height: 70rpx;
  background: rgba(191, 191, 191, 1);
  border-radius: 6rpx;
  line-height: 70rpx;
  text-align: center;
  letter-spacing: 6rpx;
  color: white;
  margin-top: 30rpx;
}

.admin_inputitems {
  display: flex;
  justify-content: center;
  margin-top: 36rpx;
}

.admin_xiaoqu {
  width: 240rpx;
  height: 60rpx;
  border: 2rpx solid rgba(236, 98, 23, 1);
  border-radius: 6rpx;
  line-height: 60rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  position: relative;
  margin-right: 25rpx;
}

.admin_xiaoqu input {
  height: 60rpx;
  width: 190rpx;
  padding: 0 10rpx;
}

.admin_xiaoquitems {
  position: absolute;
