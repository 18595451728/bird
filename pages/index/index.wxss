  z-index: 99;
  display: flex;
  flex-direction: column;
  line-height: 40rpx;
  padding: 0 10rpx;
  font-weight: bold;
  overflow-y: scroll;
}

.admin_dongitems {
  position: absolute;
  top: 62rpx;
  left: 0rpx;
  width: 173rpx;
  height: 400rpx;
  background-color: #f5f5f5;
  z-index: 99;
  display: flex;
  flex-direction: column;
  line-height: 40rpx;
  padding: 0 10rpx;
  font-weight: bold;
  overflow-y: scroll;
}

.admin_xiaoquitem {
  line-height: 48rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.admin_up {
