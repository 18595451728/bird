  flex-direction: row;
  align-items: center;
  position: relative;
}

.admin_dong input {
  height: 60rpx;
  width: 150rpx;
  padding: 0 10rpx;
}

.admin_header {
  height: 70rpx;
  width: 690rpx;
  padding-left: 30rpx;
  padding-right: 30rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  font-size: 26rpx;
  font-family: PingFangSC;
  color: rgba(78, 77, 77, 1);
  border-bottom: 1rpx solid #ddd;
}

.rightimg {
  width: 13rpx;
  height: 23rpx;
  margin-left: 19rpx;
}

.admin_change {
  display: flex;
  align-items: center;
  height: 100%;
}

.admin_items {
  width: 690rpx;
  height: 250rpx;
  background: rgba(255, 255, 255, 1);
  box-shadow: 0px 5rpx 28rpx 2rpx rgba(80, 80, 80, 0.2);
  border-radius: 4rpx;
  margin-left: 30rpx;
  margin-top: 29rpx;
  font-size: 26rpx;
  font-family: PingFangSC;
  color: rgba(78, 77, 77, 1);
  line-height: 55rpx;
}

.item_header {
  margin-left: 30rpx;
  letter-spacing: 2rpx;
  display: flex;
  align-items: center;
}

.item_msg {
  display: flex;
  flex-direction: column;
  align-items: center;
  letter-spacing: 6rpx;
}

.admin_item_state {
  width: 257rpx;
  height: 46rpx;
  background: rgba(238, 238, 238, 1);
  border-radius: 6rpx;
  font-size: 24rpx;
  font-family: PingFangSC;
  color: rgba(81, 81, 81, 1);
  letter-spacing: 2rpx;
  line-height: 46rpx;
  text-align: center;
  margin-top: 20rpx;
}

.admin_price {
  font-size: 36rpx;
  color: #eb5e2b;
}

.modal-mask {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  opacity: 0.5;
  overflow: hidden;
  z-index: 9000;
  color: #fff;
}

.mycoupon {
  position: fixed;
  width: 630rpx;
  height: 270rpx;
  left: 60rpx;
  top: 418rpx;
  background: rgba(255, 255, 255, 1);
  border-radius: 4rpx;
  z-index: 9999;
  font-size: 30rpx;
  font-family: PingFangSC;
  font-weight: 400;
  color: rgba(42, 42, 42, 1);
  display: flex;
  flex-direction: column;
  align-items: center;
}

.close {
  width: 25rpx;
  height: 25rpx;
  position: absolute;
  top: 16rpx;
  right: 16rpx;
}

.coupon_bto {
  width: 230rpx;
  height: 55rpx;
  background: linear-gradient(120deg, rgba(235, 95, 44, 1) 0%, rgba(235, 95, 43, 1) 28.000000000000004%, rgba(239, 128, 33, 1) 79%, rgba(245, 170, 51, 1) 100%);
  border-radius: 6rpx;
  /* font-size: 26rpx; */
  line-height: 55rpx;
  text-align: center;
  color: rgba(255, 255, 255, 1);
