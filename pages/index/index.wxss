page {
  background-color: white;
}

.content {
  height: 100%;
  width: 100%;
  font-size: 28rpx;
  font-family: PingFangSC;
  /* font-weight: bold; */
  color: #4d4d4d;
}

.header {
  width: 690rpx;
  height: 390rpx;
  background: rgba(255, 255, 255, 1);
  box-shadow: 0px 5rpx 28rpx 2rpx rgba(152, 152, 152, 0.35);
  border-radius: 6rpx;
  margin-left: 30rpx;
  margin-top: 30rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.header_img {
  width: 250rpx;
  height: 100rpx;
  border-radius: 6rpx;
  margin-top: 31rpx;
}

.check_on {
  border: 3rpx solid rgba(236, 98, 23, 1);
  display: flex;
  align-items: center;
  justify-content: flex-end;
  padding-right: 9rpx;
}

.check_off {
  border: 3rpx solid rgba(152, 152, 152, 1);
  display: flex;
  align-items: center;
  padding-left: 9rpx;
}

.kaiguan {
  width: 250rpx;
  /* font-weight: bold; */
  color: #eb5e2b;
  line-height: 47rpx;
  display: flex;
  justify-content: space-between;
}

.state {
  width: 265rpx;
  height: 41rpx;
  background: rgba(238, 238, 238, 1);
  border-radius: 6rpx;
  font-size: 26rpx;
  font-family: PingFangSC;
  /* font-weight: bold; */
  color: rgba(81, 81, 81, 1);
  text-align: center;
  line-height: 41rpx;
  letter-spacing: 2rpx;
}

.electricity {
  line-height: 60rpx;
  letter-spacing: 5.5rpx;
}

.header_img image {
  width: 116rpx;
  height: 82rpx;
  border-radius: 6rpx;
}

.mid {
  width: 690rpx;
  height: 140rpx;
  background: rgba(255, 255, 255, 1);
  box-shadow: 0px 5rpx 28rpx 2rpx rgba(80, 80, 80, 0.2);
  border-radius: 6rpx;
  margin-left: 30rpx;
  margin-top: 30rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
}

.miditem {
  width: 229rpx;
  height: 70rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}

.miditem image {
  height: 49rpx;
  width: 49rpx;
}

.itemmsg {
  width: 145rpx;
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  line-height: 40rpx;
}
.itemmsg>text:last-child{
  font-size: 30rpx;
}
.foot {
  width: 690rpx;
  height: 340rpx;
  background: rgba(255, 255, 255, 1);
  box-shadow: 0px 5rpx 28rpx 2rpx rgba(80, 80, 80, 0.2);
  border-radius: 6rpx;
  margin-left: 30rpx;
  margin-top: 30rpx;
}

.foot_header {
  line-height: 60rpx;
  width: 630rpx;
  height: 60rpx;
  border-bottom: 1rpx solid rgba(221, 221, 221, 1);
  margin-left: 30rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.choosetime {
  width: 630rpx;
  height: 256rpx;
  margin-left: 30rpx;
  display: flex;
  flex-direction: column;
  line-height: 50rpx;
}

.foot_header image {
  width: 50rpx;
  height: 30rpx;
}

.time_header {
  width: 418rpx;
  padding-left: 106rpx;
  display: flex;
  justify-content: space-between;
  letter-spacing: 4rpx;
  color: rgba(152, 152, 152, 1);
}

.check_time {
  width: 418rpx;
  padding-left: 106rpx;
  padding-right: 106rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  letter-spacing: 4rpx;
  background-color: #eee;
  color: #eb5e2b;
}

.time_num {
  width: 120rpx;
  height: 60rpx;
  display: flex;
  justify-content: space-between;
  line-height: 60rpx;
}

.time_henxian {
  width: 77rpx;
  height: 2rpx;
  background-color: rgba(235, 94, 43, 1);
}

.dian {
  margin-top: 18rpx;
  height: 20rpx;
  width: 4rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}

.dianitem {
  height: 4rpx;
  width: 4rpx;
  border-radius: 50%;
  background-color: rgba(235, 94, 43, 1);
}

.admin_content {
  width: 630rpx;
  margin-left: 60rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  font-size: 26rpx;
  font-family: PingFangSC;
  /* font-weight: bold; */
