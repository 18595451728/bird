}

.admin_logo {
  width: 238rpx;
  height: 233rpx;
  margin-top: 44rpx;
}

.admin_binduser {
  width: 690rpx;
  height: 80rpx;
  background: linear-gradient(120deg, rgba(235, 95, 44, 1) 0%, rgba(235, 95, 43, 1) 28.000000000000004%, rgba(239, 128, 33, 1) 79%, rgba(245, 170, 51, 1) 100%);
  border-radius: 6rpx;
  line-height: 80rpx;
  text-align: center;
  letter-spacing: 6rpx;
  color: white;
