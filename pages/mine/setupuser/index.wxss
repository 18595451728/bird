page {
  background-color: white;
  font-size: 26rpx;
  font-family: PingFangSC;
  color: rgba(78, 77, 77, 1);
}

.content {
  display: flex;
  flex-direction: column;
  align-items: center;
  height: 100%;
  width: 100%;
}

.header {
  width: 630rpx;
  height: 60rpx;
  background: rgba(238, 238, 238, 1);
  border-radius: 4rpx;
  margin-top: 30rpx;
  margin-bottom: 30rpx;
  padding: 0 30rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.icon {
  width: 33rpx;
  height: 34rpx;
}

input {
  width: 550rpx;
}

.item {
  width: 640rpx;
  background: rgba(255, 255, 255, 1);
  box-shadow: 0px 5rpx 28rpx 2rpx rgba(80, 80, 80, 0.2);
  border-radius: 4rpx;
  padding: 30rpx 20rpx 30rpx 30rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin-bottom: 20rpx;
}

.logo {
  width: 84rpx;
  height: 84rpx;
  border-radius: 50%;
}

.logo image {
  height: 100%;
  width: 100%;
}

.itembto {
  width: 177rpx;
  height: 50rpx;
  background: linear-gradient(120deg, rgba(235, 95, 44, 1) 0%, rgba(235, 95, 43, 1) 28.000000000000004%, rgba(239, 128, 33, 1) 79%, rgba(245, 170, 51, 1) 100%);
  border-radius: 6rpx;
  color: white;
  text-align: center;
  line-height: 50rpx;
}

.msg {
  display: flex;
  flex-direction: column;
  width: 320rpx;
}

.name {
  font-size: 30rpx;
  margin-bottom: 20rpx;
}

.tixing {
  width: 630rpx;
  height: 200rpx;
  background: rgba(255, 255, 255, 1);
  border-radius: 4rpx;
  position: fixed;
  top: 443rpx;
  left: 60rpx;
  z-index: 9001;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  font-size: 30rpx;
}

.close {
  width: 19rpx;
  height: 18rpx;
  position: absolute;
  top: 11rpx;
  right: 11rpx;
}

.caozuo {
  display: flex;
}

.caozuo1 {
  width: 178rpx;
  height: 50rpx;
  background: linear-gradient(120deg, rgba(235, 95, 44, 1) 0%, rgba(235, 95, 43, 1) 28.000000000000004%, rgba(239, 128, 33, 1) 79%, rgba(245, 170, 51, 1) 100%);
  border-radius: 6rpx;
  line-height: 50rpx;
  text-align: center;
  color: white;
  margin-top: 40rpx;
}

.caozuo2 {
  width: 178rpx;
  height: 50rpx;
  background: #bfbfbf;
  border-radius: 6rpx;
  line-height: 50rpx;
  text-align: center;
  color: white;
  margin-top: 40rpx;
  margin-left: 52rpx;
}
.nozuke{
  margin-top: 400rpx;
text-align: center;
font-size: 30rpx;

}