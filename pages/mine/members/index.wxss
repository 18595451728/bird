page {
  background-color: white;
}

.coupon {
  width: 630rpx;
  height: 160rpx;
  background: rgba(255, 255, 255, 1);
  box-shadow: 0px 5rpx 28rpx 2rpx rgba(80, 80, 80, 0.2);
  border-radius: 4rpx;
  margin-left: 60rpx;
  margin-top: 30rpx;
  position: relative;
  overflow: hidden;
  display: flex;
  justify-content: space-between;
}

.backimg {
  position: absolute;
  top: -70rpx;
  left: -21rpx;
  height: 260rpx;
  width: 270rpx;
}

.caozuo {
  width: 26rpx;
  height: 160rpx;
  background: rgba(235, 94, 43, 1);
  border-radius: 0px 4rpx 4rpx 0px;
  font-size: 26rpx;
  font-family: PingFangSC;
  color: rgba(255, 255, 255, 1);
  text-align: center;
  padding: 10rpx 17rpx;
}

.endtime {
  font-size: 18rpx;
  font-family: PingFangSC;
  color: rgba(152, 152, 152, 1);
  margin-left: 18rpx;
  letter-spacing: 5rpx;
}

.name {
  height: 80rpx;
  width: 510rpx;
  margin-left: 18rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  font-size: 36rpx;
  font-family: PingFangSC;
  color: rgba(77, 77, 77, 1);
  margin-top: 30rpx;
  letter-spacing: 4rpx;
}

.qixian {
  font-size: 50rpx;
  font-family: PingFangSC;
  color: rgba(235, 94, 43, 1);
  margin-right: 5rpx;
}

.vipheader {
  width: 630rpx;
  margin-left: 60rpx;
  height: 50rpx;
  border-bottom: 1rpx solid #ddd;
  display: flex;
  align-items: center;
  margin-top: 18rpx;
  font-size: 22rpx;
  font-family: PingFangSC;
  color: rgba(77, 77, 77, 1);
  letter-spacing: 4rpx;
}

.vipicon {
  width: 33rpx;
  height: 30rpx;
  margin-right: 15rpx;
}

.vipicon1 {
  width: 28rpx;
  height: 30rpx;
  margin-right: 20rpx;
}

.vipicon2 {
  width: 30rpx;
  height: 30rpx;
  margin-right: 18rpx;
  margin-top: 15rpx;
}

.vipicon3 {
  width: 30rpx;
  height: 30rpx;
  margin-right: 18rpx;
}

.username {
  border-bottom: 0;
  font-size: 26rpx;
  letter-spacing: 2rpx;
  height: 100rpx;
}

.time {
  width: 690rpx;
  border-bottom: 0;
  font-size: 26rpx;
  letter-spacing: 2rpx;
  align-items: flex-start;
}

.timeitems {
  height: 100%;
  width: 600rpx;
  display: flex;
  flex-wrap: wrap;
}

.timeitem {
  width: 117rpx;
  height: 60rpx;
  border: 2rpx solid rgba(152, 152, 152, 1);
  border-radius: 6rpx;
  margin-bottom: 30rpx;
  margin-right: 30rpx;
  line-height: 60rpx;
  text-align: center;
}

.yearinput {
  width: 60rpx;
}

.tesu {
  display: flex;
  align-items: center;
  justify-content: flex-start;
}

.mounthinput {
  width: 40rpx;
}

.bto {
  width: 630rpx;
  height: 70rpx;
  background: linear-gradient(120deg, rgba(235, 95, 44, 1) 0%, rgba(235, 95, 43, 1) 28.000000000000004%, rgba(239, 128, 33, 1) 79%, rgba(245, 170, 51, 1) 100%);
  border-radius: 6rpx;
  margin-left: 60rpx;
  text-align: center;
  line-height: 70rpx;
  font-size: 33rpx;
  font-family: PingFangSC;
  color: rgba(255, 255, 255, 1);
  margin-top: 60rpx;
}

.xufei {
  border-bottom: 0;
  font-size: 26rpx;
  letter-spacing: 2rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin-top: 150rpx;
}

.vipicon4 {
  width: 26rpx;
  height: 26rpx;
  margin-right: 8rpx;
  border-radius: 50%;
  border:1rpx solid #000 ;
  box-sizing: border-box;
}

.xufei_item {
  display: flex;
  align-items: center;
}
.xufei_item text{
  vertical-align: baseline;
}

.tishimsg {
  height: 400rpx;
  width: 630rpx;
  position: fixed;
  top: 400rpx;
  left: 60rpx;
  background-color: white;
  z-index: 9001;
  border-radius: 4rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  font-size: 36rpx;
}

.close {
  width: 26rpx;
  height: 25rpx;
  position: absolute;
  top: 16rpx;
  right: 16rpx;
}

.success {
  width: 136rpx;
  height: 136rpx;
  margin-top: 40rpx;
  margin-bottom: 20rpx;
}

.queding {
  height: 70rpx;
  background: linear-gradient(120deg, rgba(235, 95, 44, 1) 0%, rgba(235, 95, 43, 1) 28.000000000000004%, rgba(239, 128, 33, 1) 79%, rgba(245, 170, 51, 1) 100%);
  border-radius: 6rpx;
  text-align: center;
  line-height: 70rpx;
  font-size: 33rpx;
  font-family: PingFangSC;
  color: rgba(255, 255, 255, 1);
  width: 300rpx;
  margin-top: 34rpx;
}
