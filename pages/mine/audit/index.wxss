page {
  background-color: white;
  font-size: 30rpx;
  font-family: PingFangSC;
  color: rgba(77, 77, 77, 1);
}

.content {
  height: 100%;
  width: 100%;
}

.item {
  width: 690rpx;
  margin-left: 30rpx;
  border-bottom: 1rpx solid #ddd;
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 30rpx 0;
}

.icon {
  width: 44rpx;
  height: 23rpx;
  margin-left: 10rpx;
}

.room {
  font-size: 26rpx;
}
.name{
  line-height: 60rpx;
}
.time {
  font-size: 22rpx;
  color: rgba(152, 152, 152, 1);
  margin-bottom: 15rpx;
}

.caozuo {
  width: 156rpx;
  height: 44rpx;
  background: rgba(191, 191, 191, 1);
  border-radius: 6rpx;
  line-height: 44rpx;
  text-align: center;
  font-size: 26rpx;
  color: rgba(255, 255, 255, 1);
  margin-left: 20rpx;
}

.tongguo {
  background: linear-gradient(120deg, rgba(235, 95, 44, 1) 0%, rgba(235, 95, 43, 1) 28.000000000000004%, rgba(239, 128, 33, 1) 79%, rgba(245, 170, 51, 1) 100%);
}

.caozuoitem {
  display: flex;
}

.two {
  display: flex;
  flex-direction: column;
  align-items: flex-end;
}
