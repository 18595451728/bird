page {
  background-color: white;
  font-size: 26rpx;
  font-family: PingFangSC;
  color: rgba(78, 77, 77, 1);
}

.content {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.bto {
  width: 630rpx;
  height: 70rpx;
  background: linear-gradient(120deg, rgba(235, 95, 44, 1) 0%, rgba(235, 95, 43, 1) 28.000000000000004%, rgba(239, 128, 33, 1) 79%, rgba(245, 170, 51, 1) 100%);
  border-radius: 6rpx;
  font-size: 33rpx;
  color: rgba(255, 255, 255, 1);
  text-align: center;
  line-height: 70rpx;
  margin-top: 221rpx;
}

.input {
  width: 130rpx;
  height: 50rpx;
  border: 2rpx solid rgba(236, 98, 23, 1);
  border-radius: 6rpx;
  line-height: 50rpx;
  display: flex;
  justify-content: center;
  margin-left: 20rpx;
}

.input input {
  width: 70rpx;
}

.item {
  display: flex;
  align-items: center;
  margin-top: 150rpx;
  margin-bottom: 45rpx;
}

.tishimsg {
  height: 400rpx;
  width: 630rpx;
  position: fixed;
  top: 400rpx;
  left: 60rpx;
  background-color: white;
  z-index: 9001;
  border-radius: 4rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  font-size: 36rpx;
}

.close {
  width: 26rpx;
  height: 25rpx;
  position: absolute;
  top: 16rpx;
  right: 16rpx;
}

.success {
  width: 136rpx;
  height: 136rpx;
  margin-top: 40rpx;
  margin-bottom: 20rpx;
}

.queding {
  width: 300rpx;
  margin-top: 34rpx;
}
