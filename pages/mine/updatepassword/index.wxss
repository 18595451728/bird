  padding: 0 10rpx;
  margin-left: 22rpx;
}

.bto {
  width: 630rpx;
  height: 70rpx;
  background: linear-gradient(120deg, rgba(235, 95, 44, 1) 0%, rgba(235, 95, 43, 1) 28.000000000000004%, rgba(239, 128, 33, 1) 79%, rgba(245, 170, 51, 1) 100%);
  border-radius: 6rpx;
  font-size: 33rpx;
  font-family: PingFangSC;
  color: rgba(255, 255, 255, 1);
  line-height: 70rpx;
  text-align: center;
  margin-top: 150rpx;
}
.tishimsg {
  height: 400rpx;
  width: 630rpx;
  position: fixed;
  top: 400rpx;
  left: 60rpx;
  background-color: white;
  z-index: 9001;
  border-radius: 4rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  font-size: 36rpx;
}

.close {
  width: 26rpx;
  height: 25rpx;
  position: absolute;
  top: 16rpx;
