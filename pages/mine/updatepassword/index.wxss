page {
  background-color: white;
  font-size: 30rpx;
  font-family: PingFangSC;
  color: rgba(78, 77, 77, 1);
  letter-spacing: 2rpx;
}

.content {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.item {
  width: 630rpx;
  height: 60rpx;
  display: flex;
  align-items: center;
  margin-top: 30rpx;
}

.icon {
  width: 27rpx;
  height: 26rpx;
  margin-left: 20rpx;
}

.yuan {
  width: 386rpx;
  height: 60rpx;
  border: 2rpx solid rgba(236, 98, 23, 1);
  border-radius: 6rpx;
  padding: 0 10rpx;
  margin-left: 22rpx;
}

.queren {
  width: 386rpx;
  height: 60rpx;
